{
  "name": "sprinklr-manager",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-scripts": "3.4.0",
    "react-router-dom": "~5.1.2",
    "react-bootstrap": "~1.0.0-beta.16",
    "bootstrap": "~4.4.1",
    "react-icons": "~3.9.0",
    "react-csv-reader": "~2.0.2",
    "react-csv": "~2.0.1",
    "pg": "~7.18.2",
    "dotenv": "~8.2.0",
    "highcharts-react-official": "~3.0.0",
    "highcharts": "~8.0.4",
    "uuid": "~7.0.3"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "~22.3.2",
    "concurrently": "~5.1.0",
    "wait-on": "4.0.0",
    "devtron": "~1.4.0"
  },
  "scripts": {
    "start": "BROWSER=none env NODE_ENV=development concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build && electron-builder",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "electron": "electron .",
    "start:react": "react-scripts start",
    "start:electron": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "me",
  "description": "electon app for sprinklr manager",
  "build": {
    "appId": "sprinklrmanager",
    "productName": "Sprinklr Manager",
    "copyright": "Copyright",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "NSIS"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
